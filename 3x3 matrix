#include <stdio.h>
void readMatrix(int matrix[3][3]) {
    printf("Enter a 3x3 matrix:\n");
    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            printf("Enter element at position (%d, %d): ", i + 1, j + 1);
            scanf("%d", &matrix[i][j]);
        }
    }
}
void printMatrix(int matrix[3][3]) {
    printf("Matrix:\n");
    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }
}

int main() {
    int userMatrix[3][3];
    readMatrix(userMatrix);
    printMatrix(userMatrix);

    return 0;
}
